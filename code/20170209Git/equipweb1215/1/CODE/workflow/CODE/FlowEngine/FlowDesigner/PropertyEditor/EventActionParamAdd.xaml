<Window x:Class="FlowDesigner.PropertyEditor.EventActionParamAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="添加变量" Height="187" Width="339" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,14,0">
        <Label Content="变量名:" HorizontalAlignment="Left" Height="26" Margin="24,30,0,0" VerticalAlignment="Top" Width="55"/>
        <TextBox Name="Param_Name" HorizontalAlignment="Left" Height="26" Margin="84,30,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="210"/>
        <Label Content="变量值:" HorizontalAlignment="Left" Height="26" Margin="24,74,0,0" VerticalAlignment="Top" Width="55"/>
        <TextBox x:Name="Param_Value" HorizontalAlignment="Left" Height="26" Margin="84,74,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="176" RenderTransformOrigin="0.5,0.5"/>
        <Button Content="确定" HorizontalAlignment="Left" Height="30" Margin="94,116,0,0" VerticalAlignment="Top" Width="64" Click="Ok_Click"/>
        <Button Content="取消" HorizontalAlignment="Left" Height="30" Margin="188,116,0,0" VerticalAlignment="Top" Width="64" Click="Cancel_Click"/>
        <Button Content=">" HorizontalAlignment="Left" Height="26" Margin="274,74,0,0" VerticalAlignment="Top" Width="20" Click="LinkValue"/>

    </Grid>
</Window>
