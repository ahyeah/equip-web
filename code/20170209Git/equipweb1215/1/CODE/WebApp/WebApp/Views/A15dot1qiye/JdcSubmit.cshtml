        function submit_click() {

            if (!confirm('确定要保存吗？'))
                return false;
            var A15dot1_id = $("#A15dot1_id").val();



            $.ajax({
                type: 'POST',
                url: '/A15dot1/JdcModify_submitsignal',
                data: {
                    json1: '{A15dot1_id: "' + A15dot1_id
                        + '", jdcAdviseImproveMeasures: "' + $("#jdcAdviseImproveMeasures").val()
                        + '", state: "' + "2"
                        + '"}'
                },
                success: function (data) {
                    alert('操作已完成！');
                    location.href = "/A15dot1/Index";
                },
                error: function () {
                    alert('error');
                }
            });

        }
