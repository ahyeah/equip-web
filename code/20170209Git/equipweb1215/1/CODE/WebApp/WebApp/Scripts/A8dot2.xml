      </linkevents>
    </param>
    <param name="JxdwCreatePlan_done" desc="检修单位完善工单制定检修方案，完成" type="bool">
      <linkevents>
        <linkevent>JxdwCreatePlan</linkevent>
      </linkevents>
    </param>
