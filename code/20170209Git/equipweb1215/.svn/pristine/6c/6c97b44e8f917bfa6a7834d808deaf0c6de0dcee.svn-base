<Window x:Class="FlowDesigner.NewWorkFlowProj"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="新建工作流工程" Height="317" Width="385">
    <Grid>
        <Label Content="工程名称:" HorizontalAlignment="Left" Margin="49,41,0,0" VerticalAlignment="Top"/>
        <Label Content="工程路径:" HorizontalAlignment="Left" Margin="49,81,0,0" VerticalAlignment="Top"/>
        <TextBox Name="proj_name" HorizontalAlignment="Left" Height="23" Margin="115,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="161"/>
        <TextBox IsEnabled="False" Name="proj_path" HorizontalAlignment="Left" Height="23" Margin="115,83,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="161"/>
        <Button Content="..." HorizontalAlignment="Left" Margin="286,81,0,0" VerticalAlignment="Top" Width="48" Height="25" Click="Button_Click_2"/>
        <Button Content="确  认" HorizontalAlignment="Left" Margin="99,248,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="取  消" HorizontalAlignment="Left" Margin="224,248,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <GroupBox Header="数据库设置" HorizontalAlignment="Left" Margin="21,121,0,0" VerticalAlignment="Top" Height="111" Width="337">
            <Grid>
                <Label Margin="10,10,0,0" Content="服务器"/>
                <TextBox Name="server" HorizontalAlignment="Left" Margin="75,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="238" Height="23"/>
                <Label Margin="10,48,0,0" Content="数据库"/>
                <ComboBox Name="databases" HorizontalAlignment="Left" Margin="75,50,0,0" VerticalAlignment="Top" Width="238" Height="23" DropDownOpened="databases_DropDownOpened" SelectionChanged="databases_SelectionChanged"/>
            </Grid>
            
        </GroupBox>

    </Grid>
</Window>
