@model FlowEngine.UserInterface.UI_MISSION
@{
    ViewBag.Title = "CjImplementPlan";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<section class="content">
    <!-- row -->
    <div class="row">
        <div class="col-md-12">
            <!-- The time line -->
            <ul class="timeline">
                <!-- timeline time label -->
                <li class="time-label">
                     <a class="btn btn-primary" role="button" data-toggle="collapse" href="#collapseExample" aria-expanded="true" aria-controls="collapseExample">
                                @Model.WE_Event_Desc
                           </a>
                    <span class="bg-red">
                        23 DEC. 2015
                    </span>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li id="collapseExample">
                    <i class="fa fa-envelope bg-blue"></i>
                    <div class="timeline-item">
                        <span class="time"><i class="fa fa-clock-o"></i> 12:05</span>
                        <h3 class="timeline-header">设备隐患提报详情</h3>
                        <div class="timeline-body">
                            <table class="table">

                                <tbody>
                                    <tr>
                                        <td style="width:10%">申请时间</td>
                                        <td style="width:50%">@(Model.Miss_Params["submit_time"].ToString())</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">申请车间</td>
                                        <td style="width:50%">@(Model.Miss_Params["Cj_Name"].ToString())</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">申请装置</td>
                                        <td style="width:50%">@(Model.Miss_Params["Zz_Name"].ToString())</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">申请人</td>
                                        <td style="width:50%">@Model.Miss_Params["submit_user"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">设备工艺编号</td>
                                        <td style="width:50%">@Model.Miss_Params["Equip_GyCode"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">设备编号</td>
                                        <td style="width:50%">@Model.Miss_Params["Equip_Code"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">设备型号</td>
                                        <td style="width:50%">@Model.Miss_Params["Equip_Type"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">专业分类</td>
                                        <td style="width:50%">@Model.Miss_Params["Zy_Type"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">设备隐患描述</td>
                                        <td style="width:50%">@Model.Miss_Params["Problem_Desc"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td style="width:10%">隐患描述附件</td>
                                        <td style="width:50%">@Model.Miss_Params["Problem_DescFilePath"].ToString() </td>
                                    </tr>
                                        <tr>
                                        <td style="width:10%">隐患来源</td>
                                        <td style="width:50%">@Model.Miss_Params["Problem_DataSrc"].ToString() </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="timeline-footer">
                            <a class="btn btn-primary btn-xs">Read more</a>
                            <a class="btn btn-danger btn-xs">Delete</a>
                        </div>
                    </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
         
               <li class="time-label">
                    <span class="bg-green">
                        23 DEC. 2015
                    </span>
                </li>
                <li>
                    <i class="fa fa-camera bg-purple"></i>
                    <div class="timeline-item">
                        <span class="time"><i class="fa fa-clock-o"></i> 2 days ago</span>
                        <h3 class="timeline-header"><p class="lead">风险矩阵评估结果</p></h3>
                        <div class="timeline-body">
                            <form role="form" class="col-md-offset-1">                               
                                <div id="Danger_Intensity"  class="form-group"> 
                                    <p class="text-red"><strong>评估隐患强度等级为：@Model.Miss_Params["Danger_Intensity"].ToString()</strong></p>
                                </div>
                                  <div id="Time_Level"  class="form-group">
                                    <p class="text-red"><strong>评估隐患强度等级为：@Model.Miss_Params["Time_Level"].ToString()</strong></p>            
                                </div>
                            </form>
                        </div>
                    </div>
                </li>

               <li class="time-label">
                    <span class="bg-green">
                        23 DEC. 2015
                    </span>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li>
                    <i class="fa fa-camera bg-purple"></i>
                    <div class="timeline-item">
                        <span class="time"><i class="fa fa-clock-o"></i> 2 days ago</span>
                        <h3 class="timeline-header"><p class="lead">车间组织和实施应急方案</p></h3>
                        <div class="timeline-body">
                            <form role="form" >
                                <input type="text" id="wef_id"  hidden="hidden" value="@ViewBag.WF_NAME">  
                                   <div id="CjImplementedPlan"  class="form-group">
                                    <p class="text-green">车间组织和实施应急方案，是否完成</p>
                                    <div class="radio" >
                                        <label>
                                            <input name="CjImplementedPlan" id="CjImplementedPlanYes" type="radio" value="1" checked="" ">
                                            已完成 
                                        </label>
                                        <label>
                                            <input name="CjImplementedPlan" id="CjImplementedPlanNo" type="radio" value="0" ">
                                           未完成
                                        </label>  
                                       
                                    </div>
                                </div>
                        
                                <div class="box-footer">
                                    <button class="btn btn-primary" type="button" onclick="submit_click()">确定</button>
                                </div>

                            </form>
                        </div>
                    </div>
                </li>

                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                    <i class="fa fa-clock-o bg-gray"></i>
                </li>
            </ul>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
 
</section>


@section js{
<script>
    function PageLoad() {
        $("#FaultIntensity").hide();       
    };
    $(document).ready(
        PageLoad());

   
    function submit_click() {
        var flow_name = $("#wef_id").val();
        var ImplementPlan_done = "true";
        if ($("#CjImplementedPlanNo").attr("checked"))
            ImplementPlan_done = "false";

        $.ajax({
            type: 'POST',
            url: '/A11dot2/submitImplementPlan_signal',
            data: {
                json1: '{Flow_Name: "' + flow_name
                        + '", ImplementPlan_done: "' + ImplementPlan_done + '"}'
            },
            success: function (data) {
                location.href = data;
            },
            error: function () {
                alert('error');
            }
        });

    }
    </script>
}

