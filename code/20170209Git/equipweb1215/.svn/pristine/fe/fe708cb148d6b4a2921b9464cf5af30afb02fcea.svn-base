@section js{
<script>

    $(function () {
        //submit_click();

    });

    function submit_click() {
        var flow_name = $("#wef_id").val();
        var module_name = $("#module_name").val();
        $.ajax({
            type: 'POST',
            url: '/A13dot1/submitOtherModule_signal',
            data: {
                json1: '{Flow_Name: "' + flow_name
                   + '", module_name: "' + module_name + '"}'
            },
            success: function (data) {
                location.href = data;
            },
            error: function () {
                alert('error');
            }
        });
    }

    </script>
}

