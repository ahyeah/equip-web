@model WebApp.Controllers.A13dot1Controller.WFDetail_Model
@{
    ViewBag.Title = "ZytdConfirm";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<section class="content">
    <!-- row -->
    <div class="row">
        <div class="col-md-12">
            <!-- The time line -->
            <ul class="timeline">
                <!-- timeline time label -->
                <li class="time-label">
                    <a class="btn btn-primary" role="button" data-toggle="collapse" href="#collapseExample" aria-expanded="true" aria-controls="collapseExample">
                        @Model.ALLHistoryMiss[1].WE_Event_Desc
                        </a>
</li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li class="collapse.in" id="collapseExample">
                    <i class="fa fa-envelope bg-blue"></i>
                    <div class="timeline-item">
                        <i class="fa fa-clock-o"></i> 提报人：@Model.MissUser[1]
                        <h3 class="timeline-header"><p class="lead">装置缺陷提报详情</p></h3>
                        <div class="timeline-body">
                            <table class="table">
                                <tbody>
                                    <tr>
                                        <td>申请时间</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["submit_time"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>申请车间</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Cj_Name"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>申请装置</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Zz_Name"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>申请人</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["submit_user"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备工艺编号</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Equip_GyCode"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备编号</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Equip_Code"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备型号</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Equip_Type"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>问题专业分类</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Zy_Type"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备问题描述</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Problem_Desc"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>问题描述附件</td>
                                        <td>@Model.ALLHistoryMiss[1].Miss_Params["Problem_DescFilePath"].ToString()</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="timeline-footer">
                            <a class="btn btn-primary btn-xs">Read more</a>
                            <a class="btn btn-danger btn-xs">Delete</a>
                        </div>
                    </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->

                <li class="time-label">
                    <a class="btn btn-primary" role="button" data-toggle="collapse" href="#collapseExample1" aria-expanded="true" aria-controls="collapseExample1">
                        @Model.ALLHistoryMiss[2].WE_Event_Desc
                    </a>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li class="collapse" id="collapseExample1" >
                    <i class="fa fa-camera bg-purple"></i>
                    <div class="timeline-item">
                        <span class="time"><i class="fa fa-clock-o"></i>分类用户： @Model.MissUser[2]</span>
                        <h3 class="timeline-header"><p class="lead">片区分类</p></h3>
                        <div class="timeline-body">
                            <table class="table">
                                <tbody>
                                    <tr>
                                        <td>分类结果</td>
                                        <td>@Model.ALLHistoryMiss[2].Miss_Params["catlog_type"].ToString()</td>
                                    </tr>
                                    @if (@Model.ALLHistoryMiss[2].Miss_Params["catlog_type"].ToString() != "不需处理")
                                    {
                                    <tr>
                                        <td>缺陷等级</td>
                                        <td>@Model.ALLHistoryMiss[2].Miss_Params["defect_level"].ToString()</td>
                                    </tr>
                                    }
                                    @if (@Model.ALLHistoryMiss[2].Miss_Params["catlog_type"].ToString() == "故障")
                                    {
                                    <tr>
                                        <td>故障强度等级</td>
                                        <td id="fault_intensity_pq">@Model.ALLHistoryMiss[2].Miss_Params["fault_intensity"].ToString()</td>
                                    </tr>
                                    }
                                </tbody>
                            </table>

                        </div>
                    </div>
                </li>

                @for (int i = 3; i < @Model.ALLHistoryMiss.Count(); i++)
{     <li class="time-label">
          <a class="btn btn-primary" role="button" data-toggle="collapse" href="#@i" aria-expanded=true aria-controls="@i">
              @Model.ALLHistoryMiss[i].WE_Event_Desc
          </a>
                </li>
      <li class="collapse" id="@i">
        <i class="fa fa-camera bg-purple"></i>
        <div class="timeline-item">
            <span class="time"><i class="fa fa-clock-o"></i>提交用户： @Model.MissUser[i]</span>
            <h3 class="timeline-header"><p class="lead">@Model.ALLHistoryMiss[i].WE_Event_Desc</p></h3>
            <div class="timeline-body">
                <table class="table">
                    <tbody>
                        @foreach (var pp in @Model.ALLHistoryMiss[i].Miss_Params)
                        {
                        <tr>
                            <td>@pp.Key</td>
                            <td>@pp.Value.ToString()</td>
                        </tr>
                        }

                    </tbody>
                </table>

            </div>
        </div>
    </li>
}
                     


            
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                    <i class="fa fa-clock-o bg-gray"></i>
                </li>
            </ul>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->

</section>


@section js{
    <script>
        function PageLoad() {
            $("#FaultIntensity").hide();
        };
        $(document).ready(
            PageLoad());

        function ZjtdOpinionNo_Click() {
            $("#FaultIntensity").show();

        }
        function ZjtdOpinionYes_Click() {
            $("#FaultIntensity").hide();

        }

        function submit_click() {
            var flow_name = $("#wef_id").val();
            var fault_intensity = $("#fault_intensity_pq").val();
            if ($("#ZjtdOpinionNo:checked").val())
                fault_intensity = $(".checkbox-inline:input:radio:checked").val();
            $.ajax({
                type: 'POST',
                url: '/A13dot1/submitZytdConfirm_signal',
                data: {
                    json1: '{Flow_Name: "' + flow_name
                        + '", fault_intensity: "' + fault_intensity + '"}'
                },
                success: function (data) {
                    location.href = data;
                },
                error: function () {
                    alert('error');
                }
            });

        }
    </script>
}

