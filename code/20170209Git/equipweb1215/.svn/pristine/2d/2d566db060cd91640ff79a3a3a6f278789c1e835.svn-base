@model WebApp.Controllers.A11dot2Controller.CatalogModel
@{
    ViewBag.Title = "ZytdConfirm";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<section class="content">
    <!-- row -->
    <div class="row">
        <div class="col-md-12">
            <!-- The time line -->
            <ul class="timeline">
                <!-- timeline time label -->
                <li class="time-label">
                    <a class="btn btn-primary" role="button" data-toggle="collapse" href="#collapseExample" aria-expanded="true" aria-controls="collapseExample">
                       装置隐患提报
                       </a>
              </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li class="collapse.in" id="collapseExample">
                    <i class="fa fa-envelope bg-blue"></i>
                    <div class="timeline-item">
                        <i class="fa fa-clock-o"></i> 提报人：@Model.MissUser[1]  提报时间：@Model.MissTime[1]
                        <h3 class="timeline-header"><p class="lead">装置隐患提报详情</p></h3>
                        <div class="timeline-body">
                            <table class="table">
                                <tbody>
                                    <tr>
                                        <td>申请时间</td>
                                        <td>@Model.miModel.Miss_Params["submit_time"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>申请车间</td>
                                        <td>@Model.miModel.Miss_Params["Cj_Name"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>申请装置</td>
                                        <td>@Model.miModel.Miss_Params["Zz_Name"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>申请人</td>
                                        <td>@Model.miModel.Miss_Params["submit_user"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备工艺编号</td>
                                        <td>@Model.miModel.Miss_Params["Equip_GyCode"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备编号</td>
                                        <td>@Model.miModel.Miss_Params["Equip_Code"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备型号</td>
                                        <td>@Model.miModel.Miss_Params["Equip_Type"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>问题专业分类</td>
                                        <td>@Model.miModel.Miss_Params["Zy_Type"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>设备问题描述</td>
                                        <td>@Model.miModel.Miss_Params["Problem_Desc"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>问题描述附件</td>
                                        <td>@Model.miModel.Miss_Params["Problem_DescFilePath"].ToString()</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="timeline-footer">
                            <a class="btn btn-primary btn-xs">Read more</a>
                            <a class="btn btn-danger btn-xs">Delete</a>
                        </div>
                    </div>
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->

                <li class="time-label">
                    <a class="btn btn-primary" role="button" data-toggle="collapse" href="#collapseExample1" aria-expanded="false" aria-controls="collapseExample1">
                       片区风险矩阵评估
                    </a>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li class="collapse.in" id="collapseExample1">
                    <i class="fa fa-camera bg-purple"></i>
                    <div class="timeline-item">
                        <span class="time"><i class="fa fa-clock-o"></i>评估用户： @Model.MissUser[2] 评估结果提报时间：@Model.MissTime[1]</span>
                        <h3 class="timeline-header"><p class="lead">片区风险矩阵评估结果</p></h3>
                        <div class="timeline-body">
                            <table class="table">
                                <tbody>
                                    <tr>
                                        <td>隐患强度等级：</td>
                                        <td>@Model.miModel.Miss_Params["Danger_Intensity"].ToString()</td>
                                    </tr>
                                    <tr>
                                        <td>时间等级</td>
                                        <td>@Model.miModel.Miss_Params["Time_Level"].ToString()</td>
                                    </tr>
                        
                                </tbody>
                            </table>

                        </div>
                    </div>
                </li>


                <li class="time-label">
                    <span class="bg-green">
                        @ViewBag.curtime
                    </span>
                </li>
                <!-- /.timeline-label -->
                <!-- timeline item -->
                <li>
                    <i class="fa fa-camera bg-purple"></i>
                    <div class="timeline-item">
                        <span class="time"><i class="fa fa-clock-o"></i>审核人: @ViewBag.curuser</span>
                        <h3 class="timeline-header"><p class="lead">专家团队审核</p></h3>
                        <div class="timeline-body">
                            <form role="form">
                                <input type="text" id="wef_id" hidden="hidden" value="@ViewBag.WF_NAME">
                                <div id="ZjtdOpinion" class="form-group">
                                    <p class="text-green">专家团队审核意见</p>
                                    <div class="radio">
                                        <label>
                                            <input name="ZjtdOpinion" id="ZjtdOpinionYes" type="radio" value="1" checked="" onchange="ZjtdOpinionYes_Click()">
                                            同意
                                        </label>
                                        <label>
                                            <input name="ZjtdOpinion" id="ZjtdOpinionNo" type="radio" value="0" onchange="ZjtdOpinionNo_Click()">
                                            不同意(需重新设定）
                                        </label>

                                    </div>
                                </div>
                                <div id="FaultIntensity" class="form-group">
                                                              
                                <div id="Danger_Intensity"  class="form-group"> 
                                    <p class="text-red"><strong>评估隐患强度等级为：</strong></p>
                                        <div >
                                        <label class="radio">
                                            <input type="radio" name="intensityoptionsRadios" id="DangerIntensity1" value="S1" >S1 - 全厂生产波动，2套以上装置非计划停工或全厂生产降量
                                        </label>
                                          <label class="radio">
                                            <input type="radio" name="intensityoptionsRadios" id="DangerIntensity2" value="S2" >S2 - 单套装置非计划停工或2套以上装置异常波动
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="intensityoptionsRadios" id="DangerIntensity3" value="S3" >S3 - 系统或装置局部停工，大机组急停
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="intensityoptionsRadios" id="DangerIntensity4" value="S4" >S4 - 单套生产装置异常波动
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="intensityoptionsRadios" id="DangerIntensity5" value="S5" >S5 - 单台设备停运
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="intensityoptionsRadios" id="DangerIntensity6" value="S6" >S6 - 无影响
                                        </label>                                
                                       </div>
                                </div>
                                  <div id="Time_Level"  class="form-group">
                                    <p class="text-red"><strong>评估时间等级为：</strong></p>
                                        <div >
                                        <label class="radio">
                                            <input type="radio" name="leveloptionsRadios" id="TimeLevel1" value="D1" >D1 - 10天内可能发生
                                        </label>
                                          <label class="radio">
                                            <input type="radio" name="leveloptionsRadios" id="TimeLevel2" value="D2" >D2 - 10天至1个月内可能发生
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="leveloptionsRadios" id="TimeLevel3" value="D3" >D3 - 1个月至3个月内可能发生
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="leveloptionsRadios" id="TimeLevel4" value="D4" >D4 - 3个月至6个月内可能发生
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="leveloptionsRadios" id="TimeLevel5" value="D5" >D5 - 6个月至1年内可能发生
                                        </label>
                                              <label class="radio">
                                            <input type="radio" name="leveloptionsRadios" id="TimeLevel6" value="D6" >D6 - 1年内不会发生
                                        </label>                                
                                       </div>
                                </div>
                  
           
                                </div>
                                <div class="box-footer">
                                    <button class="btn btn-primary" type="button" onclick="submit_click()">确定</button>
                                </div>

                            </form>
                         </div>
                        </div>
                    
                </li>
                <!-- END timeline item -->
                <!-- timeline item -->
                <li>
                    <i class="fa fa-clock-o bg-gray"></i>
                </li>
            </ul>
        </div>
        <!-- /.col -->
      </div>
    <!-- /.row -->

</section>



@section js{
<script>
    function PageLoad() {
        $("#FaultIntensity").hide();       
    };
    $(document).ready(
        PageLoad());

    function ZjtdOpinionNo_Click()
    {
      $("#FaultIntensity").show();     
  
    }
    function ZjtdOpinionYes_Click() {
        $("#FaultIntensity").hide();

    }
   
    function submit_click() {
        var flow_name = $("#wef_id").val();
        var Confirm_result = "true";
        if ($("#ZjtdOpinionNo").attr("checked"))
            Confirm_result = "false";

        $.ajax({
            type: 'POST',
            url: '/A11dot2/submitZytdConfirm_signal',
            data: {
                json1: '{Flow_Name: "' + flow_name
                        + '", Confirm_result: "' + Confirm_result + '"}'
            },
            success: function (data) {
                location.href = data;
            },
            error: function () {
                alert('error');
            }
        });

    }
    </script>
}



