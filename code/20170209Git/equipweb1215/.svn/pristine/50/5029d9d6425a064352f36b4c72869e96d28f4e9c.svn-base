@model WebApp.Controllers.CommonController.WFDetail_Model

@{
    ViewBag.Title = "PqCatalog";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<section class="content">
    <!-- row -->
    <div class="row">
        <div class="col-md-12">
                <!-- The time line -->
                <ul class="timeline">  
                     <!-- 工作流当前任务-Star -->
                    <!-- timeline item -->
                     <li class="time-label">
                        <span class="bg-green">
                           业务流水号：<button class="btn btn-default"  onclick="DispalsySide('@ViewBag.WF_Ser')">@ViewBag.WF_Ser</button>
                        </span>
                    </li>
                    <li class="time-label">
                        <span class="bg-green">
                           当前任务：缺陷分类
                        </span>
                    </li>
                    <!-- /.timeline-label -->
                    <!-- timeline item -->
                    <li>
                        <i class="fa fa-camera bg-purple"></i>
                        <div class="timeline-item">
                            <span class="time"><i class="fa fa-clock-o">@ViewBag.curtime</i>当前用户：@ViewBag.curuser</span>
                            <h3 class="timeline-header"><span class="lead" style="color:red">缺陷分类</span></h3>
                            <div class="timeline-body">
                                <input type="text" id="wef_id" hidden="hidden" value="@ViewBag.WF_NAME">
                                <div class="form-group">
                                    <p class="text-green">选择缺陷类别</p>
                                    <select id="qqq" onchange="FaultLvel_Click()" class="form-control">
                                        <option value="完好类">完好类</option>
                                        <option value="隐患">隐患</option>
                                        <option value="故障">故障</option>
                                        <option value="不需处理" selected="">不需处理</option>
                                    </select>
                                </div>
                                <div id="FaultLevel" class="form-group">
                                    <p class="text-green">选择缺陷级别</p>
                                    <div class="radio">
                                        <label>
                                            <input name="defect_level_sel" id="FaultLevel_rad1" type="radio" value="一级缺陷">
                                            一级缺陷
                                        </label>
                                        <label>
                                            <input name="defect_level_sel" id="FaultLevel_rad2" type="radio" value="二级缺陷">
                                            二级缺陷
                                        </label>

                                        <label>
                                            <input name="defect_level_sel" id="FaultLevel_rad3" type="radio" value="三级缺陷">
                                            三级缺陷
                                        </label>
                                    </div>
                                </div>
                                <div id="FaultIntensity" class="form-group">
                                    <p class="text-green">选择故障强度级别</p>
                                    <select class="form-control" id="fault_Intensity_level">
                                        <option value="1级">1级</option>
                                        <option value="2级">2级</option>
                                        <option value="3级">3级</option>
                                        <option value="4级">4级</option>
                                        <option value="5级">5级</option>
                                        <option value="6级">6级</option>
                                    </select>
                                </div>
                                <div class="box-footer">
                                    <button class="btn btn-primary" type="button" onclick="submit_click()">确定</button>
                                   @* <button class="btn btn-primary col-sm-offset-1" type="button" onclick="cancel_click()">取消</button>*@
                                </div>


                            </div>
                        </div>
                    </li>
                    <!-- END timeline item -->
                    <!-- timeline item -->
                 <!-- 工作流当前任务-End -->
                  
                 <!-- 工作流历史操作-通用部分Star -->
                   <li class="time-label">
                            <span class="bg-green">
                               当前工作流历史操作
                            </span>
                        </li>
                     <!-- timeline time label -->  
                    @for (int i = @Model.ALLHistoryMiss.Count()-1; i >=1 ; i--)
                    {     <li class="time-label">
                              <a class="btn btn-primary" role="button" data-toggle="collapse" href="#@i" aria-expanded=true aria-controls="@i">
                                  @Model.MissTime[i] - @Model.ALLHistoryMiss[i].WE_Event_Desc
                              </a>
                           </li>
                    
                          <li class="collapse" id="@i">
                            <i class="fa fa-camera bg-purple"></i>
                            <div class="timeline-item">
                                <span class="time"><i class="fa fa-clock-o">@Model.MissTime[i]</i>操作用户： @Model.MissUser[i]</span>
                                <h3 class="timeline-header lead"><span class="lead" style="color:red">@Model.ALLHistoryMiss[i].WE_Event_Desc</span></h3>
                                <div class="timeline-body">
                                    <table class="table  table-hover table-bordered">
                                        <tbody>
                                             @foreach (var pp in @Model.ALLHistoryMiss[i].Miss_ParamsAppRes)
                                            {
                                                if(pp.Value.ToString()!="App_hidden")
                                                {
                                                    <tr class="info">
                                                        <th>@Model.ALLHistoryMiss[i].Miss_ParamsDesc[pp.Key]</th>
                                                       @if ((@Model.ALLHistoryMiss[i].Miss_ParamsDesc[pp.Key].ToString()).IndexOf("附件") > -1 && @Model.ALLHistoryMiss[i].Miss_Params[pp.Key]!="")
                                                       {
                                                           string[] savedFileName = (@Model.ALLHistoryMiss[i].Miss_Params[pp.Key].ToString()).Split(new char[] { '$' });
                                                           string saveExistFilename = Path.Combine("/Upload", savedFileName[1]);
                                                           <td><a href=@saveExistFilename target=_blank> @savedFileName[0]</a></td> }
                                                       else
                                                        {<td>@Model.ALLHistoryMiss[i].Miss_Params[pp.Key]</td>}
                                                    </tr>
                                                 }
                                            }
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </li>
                }
                     
                <!-- END timeline item -->

                  <li>
                        <i class="fa fa-clock-o bg-gray"></i>
                    </li> 
            <!-- 工作流历史操作-通用部分End -->

           </ul>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->

</section>

<!--参数显示模态框开始处-->
<div class="modal fade" id="ParamModal">
    <div class="modal-dialog ">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">相关参数显示</h4>
            </div>
            <div class="modal-body">
                <div class="modal-body" id="ParamDiv">
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-warning" data-dismiss="modal">关闭</button>

            </div>
        </div>

    </div>

</div>
<!--参数显示模态框结束处--> 
@section js{
       <link href="~/Plugins/AdminLTE/plugins/bootstrap-treeview/bootstrap-treeview.min.css" rel="stylesheet" />
    <script src="~/Plugins/AdminLTE/plugins/bootstrap-treeview/bootstrap-treeview.min.js"></script>
        <script src="~/Scripts/myWorkFlowInfo.js"></script>
    <script>
        function PageLoad() {
            // $("#FaultLevel").hide();
            /*  $("#optionsRadios1").attr("check", "false");
              $("#optionsRadios1").attr("disabled", "true");
              $("#optionsRadios2").attr("checked", "false");
              $("#optionsRadios2").attr("disabled", "true");
              $("#optionsRadios3").attr("checked", "true");
              $("#optionsRadios3").attr("disabled", "true");
              $("#FaultIntensity").hide();*/
            $("#FaultLevel").hide();
            $("#FaultIntensity").hide();

        };
        $(document).ready(
            PageLoad());

        function FaultLvel_Click() {
            //$("#FaultLevel").show();
            //$("#FaultIntensity").show();
            var str = $("#qqq").val();
            // var str =$("FaultLevel_sel option[selected=selected]");
            // alert(str);
            if (str == "完好类") {
                $("input:radio[name='defect_level_sel']").eq(1).attr("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(0).attr("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(2).attr("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(2).prop("checked", true);
                $("input:radio[name='defect_level_sel']").eq(2).attr("disabled", true);
                $("input:radio[name='defect_level_sel']").eq(1).attr("disabled", true);
                $("input:radio[name='defect_level_sel']").eq(0).attr("disabled", true);

                $("#FaultLevel").show();
                $("#FaultIntensity").hide();

            }
            else if (str == "隐患") {

                $("input:radio[name='defect_level_sel']").eq(1).prop("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(0).prop("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(2).attr("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(1).prop("checked", true);
                $("input:radio[name='defect_level_sel']").eq(0).attr("disabled", true);

                $("input:radio[name='defect_level_sel']").eq(2).attr("disabled", false);
                $("#FaultLevel").show();
                $("#FaultIntensity").hide();


            } else if (str == "故障") {
                $("input:radio[name='defect_level_sel']").eq(1).attr("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(0).attr("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(2).attr("disabled", false);
                $("input:radio[name='defect_level_sel']").eq(0).prop("checked", true);
                $("input:radio[name='defect_level_sel']").eq(2).attr("disabled", true);
                $("input:radio[name='defect_level_sel']").eq(1).attr("disabled", true);
                $("input:radio[name='defect_level_sel']").eq(0).attr("disabled", true);
                $("#FaultLevel").show();
                $("#FaultIntensity").show();

            } else {
                $("#FaultLevel").hide();
                $("#FaultIntensity").hide();
            }

        }

        function submit_click() {
            var flow_name = $("#wef_id").val();

            $.ajax({
                type: 'POST',
                url: '/A13dot1/PqCatalog_submitsignal',
                data: {
                    json1: '{Flow_Name: "' + flow_name
                         + '", catlog_type: "' + $('#qqq').val()
                         + '", fault_intensity: "' + $('#fault_Intensity_level').val()
                         + '", defect_level: "' + $("input:radio[name='defect_level_sel']:checked").val()
                         + '"}'
                },
                success: function (data) {
                    location.href = data;
                    //alert(data);
                },
                error: function (data) {
                    alert(data);
                }
            });

        }


    </script>
}
